{
  "name": "@createiq/slidev-theme",
  "version": "0.0.2",
  "description": "CreateiQ Slidev theme",
  "author": "CreateiQ <support@createiq.tech>",
  "license": "MIT",
  "homepage": "https://createiq.tech",
  "repository": {
    "type": "git",
    "url": "https://github.com/linklaterscreateiq/slidev-theme-createiq"
  },
  "bugs": "https://github.com/linklaterscreateiq/slidev-theme-createiq/issues",
  "keywords": [
    "slidev-theme",
    "slidev",
    "createiq"
  ],
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.47.0"
  },
  "slidev": {
    "colorSchema": "light",
    "defaults": {
      "fonts": {
        "sans": "Lato",
        "local": "Lato"
      }
    }
  },
  "scripts": {
    "dev": "slidev example.md --open",
    "build": "slidev build example.md",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png",
    "prepare": "husky"
  },
  "dependencies": {
    "@slidev/types": "^52.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@slidev/cli": "52.11.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "playwright-chromium": "1.58.0",
    "vue": "3.5.25"
  },
  "lint-staged": {
    "*": [
      "biome check --write --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  }
}
